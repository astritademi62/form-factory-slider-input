package fcnt_rangeSliderInputDefinition.html

input {
    label "Range Slider Input"
    template "<ff-range-slider-input></ff-range-slider-input>"
    wizard "<ff-range-slider-input view-type='designView'></ff-range-slider-input>"
    supportedLogicTypes "valid", "input"
    properties {
            translate "default"
        }
    propertiesI18n {
        helptext "help"
        minValue "10"
        maxValue "90"
        floor "0"
        ceil "100"
        ticks "false"
        customTicks "1"
        step "1"
    }
    resources {
        css "css/rzslider.css"
        javascript "javascript/rzslider.js"
    }
}